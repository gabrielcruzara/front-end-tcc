{
  "name": "",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build:dev": "npm run env -s && ng build --configuration development",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --proxy-config proxy.conf.js",
    "start:pwa": "ng build --prod && http-server -p 8080 -c-1 dist",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort",
    "compodoc": "compodoc -p tsconfig.json -d docs/generated -s -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^11.2.6",
    "@angular/cdk": "^10.1.4",
    "@angular/common": "^11.2.6",
    "@angular/compiler": "^11.2.6",
    "@angular/core": "^11.2.6",
    "@angular/forms": "^11.2.6",
    "@angular/localize": "^11.2.6",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^11.2.6",
    "@angular/platform-browser-dynamic": "^11.2.6",
    "@angular/router": "^11.2.6",
    "@angular/service-worker": "^11.2.6",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "@ngx-loading-bar/core": "^4.2.0",
    "@ngx-loading-bar/http-client": "^4.2.0",
    "@ngx-loading-bar/router": "^4.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "admin-lte": "^3.0.5",
    "angular-confirmation-popover": "^5.0.0",
    "bootstrap": "^4.5.3",
    "chart.js": "^2.9.3",
    "ionicons": "2.0.1",
    "jquery": "^3.5.1",
    "jquery-mask-plugin": "^1.14.16",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "ng2-charts": "^2.2.3",
    "ngx-bootstrap": "^5.6.1",
    "ngx-currency": "^2.5.3",
    "ngx-google-analytics": "^11.1.0",
    "ngx-gravatar": "^10.0.0",
    "ngx-guided-tour": "^1.1.11",
    "ngx-joyride": "^2.4.0",
    "ngx-mask": "^11.1.5",
    "ngx-spinner": "^10.0.1",
    "ngx-toastr": "^12.0.1",
    "rxjs": "^6.6.3",
    "sweetalert2": "^9.17.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.5",
    "@angular/cli": "^11.2.14",
    "@angular/compiler-cli": "^11.2.6",
    "@angular/language-service": "^11.2.6",
    "@angularclass/hmr": "^3.0.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/jquery": "^3.5.5",
    "@types/jquery-mask-plugin": "^1.14.3",
    "@types/lodash": "^4.14.161",
    "@types/node": "^12.12.62",
    "codelyzer": "^6.0.0",
    "htmlhint": "^0.14.1",
    "https-proxy-agent": "^5.0.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "stylelint": "~13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.18.0",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.3"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
